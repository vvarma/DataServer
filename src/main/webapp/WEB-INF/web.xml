<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-indexName>Archetype Created Web Application</display-indexName>
    <filter>
        <filter-indexName>TeeFilter</filter-indexName>
        <filter-class>ch.qos.logback.access.servlet.TeeFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-indexName>TeeFilter</filter-indexName>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-indexName>ViewStatusMessages</servlet-indexName>
        <servlet-class>ch.qos.logback.classic.ViewStatusMessagesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-indexName>ViewStatusMessages</servlet-indexName>
        <url-pattern>/logbackStatus</url-pattern>
    </servlet-mapping>
</web-app>
